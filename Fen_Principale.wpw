  procedures :
   -
     name : Inventorier
     procedure_id : 1626482859213318591
     type_code : 14
     code : |1+
      Procédure Inventorier(local CodeProduit)
      
      SI CodeProduit = 0 ALORS
      	// Aucun code produit n'a été saisi
      	Erreur(<§$001b§>)
      	RepriseSaisie(SAI_CodeBarres)
      FIN
      
      nLigne est un entier
      nLigne = ZoneRépétéeCherche(att_Code,CodeProduit)
      
      
      SI nLigne <> -1 ALORS 
      	
      	//On se positionne sur le bon produit dans la base de données	
      	HLitRecherchePremier(PRODUIT,CodeBarre,CodeProduit)	
      	// On ouvre la fenêtre permettant de saisir la quantité de produits inventoriés
      	OuvreFenêtreMobile(FEN_Quantité)	
      	
      SINON
      	// Le code qui vient d'être scanné ou saisi : il n'existe pas dans notre base des produits.
      	// Je vais proposer à l'utilisateur de créer la fiche ... après lui avoir posé la bonne question ..
      	SI OuiNon (chaineconstruit(<§$001c§>,CodeProduit)+RC+<§$001a§>) ALORS
      		// Si l'utilisateur valide l'ajout, je rafraîchis la table ...
      		OuvreFenêtreMobile(fen_CreeFicheProduit,CodeProduit)
      	fin
      FIN
      
